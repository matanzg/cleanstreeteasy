{
    "name": "Clean Street Easy",
    "description": "Remove all 'Featured' and 'Sponsored' properties from StreetEasy search results.",
    "version": "0.9",
    "manifest_version": 3,
    "content_scripts": [
        {
            "matches": [
                "https://streeteasy.com/for-*"
            ],
            "js": [
                "content.js"
            ]
        }
    ],
    "icons": {
        "16": "/images/logo_16.png",
        "32": "/images/logo_32.png",
        "48": "/images/logo_48.png",
        "128": "/images/logo_128.png"
    }
}